    "postcss-html": "^1.7.0",
    "react-i18next": "^15.4.1",
    "standard-version": "^9.5.0",
    "start-server-and-test": "^2.0.8",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript-eslint": "^8.12.2",
