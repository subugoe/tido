    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@types/openseadragon": "^3.0.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
