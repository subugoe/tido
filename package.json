  "browserslist": [
    "last 1 version",
    "not dead",
    "not IE 11",
    "> 5%"
  ],
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 6.13.4"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:embed",
    "build:lib": "vite build && cp src/types.d.ts dist/types.d.ts",
    "build:embed": "vite build --config vite.embed.config.js",
    "build:test": "vite build --config vite.embed.config.js --mode test",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --headless",
    "dev": "node vite-start.js",
    "fix": "npx eslint --fix src ",
    "lint": "npx eslint src",
    "release": "npm run build && npm publish",
    "api": "http-server tests/mocks -p 8181 -c-1 --silent --cors",
    "examples": "http-server examples -p 2222 -c-1 --silent --cors",
    "preview": "npm run build && vite preview",
    "preview:examples": "npm run build:test && cp -r dist/ examples/dist/ && npm run examples",
    "test": "start-server-and-test preview:examples 2222 cypress:headless",
    "write-changelog": "standard-version"
  },
  "files": [
    "src",
    "dist"
  ]
}
